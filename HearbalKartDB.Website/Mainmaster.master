    <style type="text/css">
        #Loding_overlay {
            display: none;
            position: absolute;
            top: 0%;
            left: 0%;
            width: 100%;
            height: 1000%;
            background-color: #FFF;
            z-index: 1001;
            -moz-opacity: 0.7;
            opacity: .70;
            filter: alpha(opacity=70);
            text-align: center;
        }

        .fk-ui-goTop {
